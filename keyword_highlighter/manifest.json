{
  "manifest_version": 3,
  "name": "Keyword Highlighter",
  "version": "1.0",
  "description": "Highlights uppercase and capitalized words.",
  "icons": {
    "48": "icons/icon.svg",
    "96": "icons/icon.svg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
